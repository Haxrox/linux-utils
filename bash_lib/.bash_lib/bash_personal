
BASH_LIB_ROOT=$(dirname "${BASH_SOURCE[0]}")

# Libraries
for lib in $(ls -d ${BASH_LIB_ROOT}/*/); do
  source "${lib}/$(basename ${lib}).sh"
done

# Aliases

# Code
function main() {
  setPrompt "\[$(echo '${PROMPT_PREPEND}')\]\[${BGreen}\][\@]\[${Red}\]\[$(echo '$(__git_ps1 ["%s"])')$(echo '${PROMPT_APPEND}')\]\[${BBlue}\]\w/\[${Color_Off}\]$ "
}

main "$@"